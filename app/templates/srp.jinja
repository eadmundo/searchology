{% from '_includes/search_macros.jinja' import pagination_ui %}
<p>There are {{ pagination.total_count }} results</p>
<ul>
{% for result in pagination.items %}
    <li><a href="{{ result['fields']['url'] }}">{{ result['fields']['url'] }}</a></li>
{% endfor %}
</ul>
{{ pagination_ui(pagination.pages, pagination.page, qstr) }}